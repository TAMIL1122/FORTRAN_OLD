!1234 PROGRAM FOR NEWTON RAPHSON METHOD
DIMENSION X(100)
F(X)=EXP(2*X)-EXP(X)-2
FD(X)=2*EXP(2*X)-EXP(X)
WRITE(*,*)'ENTER THE INITIAL GUESS VALUE'
READ(*,5)X(1)
5 FORMAT (F15.5)
WRITE(*,*)'ITERATION,X,F(X):'
N=1
50 X(N+1)=X(N)-(F(X(N))/FD(X(N)))
WRITE(*,10)N+1,X(N+1),F(X(N+1))
10 FORMAT (1X,I6,5X,F15.5,5X,F15.5)
IF (ABS((X(N+1))-X(N)).LE.0.00005) GO TO 100
N=N+1
GO TO 50
100 WRITE(*,15)X(N+1)
15 FORMAT(1X,'THE ROOT OF THE GIVE POLYNOMIAL IS:',F15.5)
STOP
END  

 